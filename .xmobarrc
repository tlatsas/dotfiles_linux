Config { font = "xft:Sans Mono:size=8"
       , bgColor = "#232323"
       , fgColor = "#9fbc00"
       , position = BottomW L 90
       , lowerOnStart = True
       , commands = [ Run Weather "LGAV" ["-t"," <tempC>C","-L","10","-H","35","--normal","green","--high","red","--low","lightblue"] 36000
                    , Run Date "%a %b %d %H:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ <fc=#ee9a00>%date%</fc> | Athens:%LGAV%"
       }
