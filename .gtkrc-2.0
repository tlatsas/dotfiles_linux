# gtk application style

# theme
gtk-theme-name = "Shiki-Brave"

# mouse cursor
gtk-cursor-theme-name = "ComixCursors-White-Small-Slim"

# icons
gtk-icon-theme-name = "Faenza-Darkest"

# font
style "font"
{
    font_name = "Sans 8"
# include rc style for the xfce4-notes-plugin
}
widget_class "*" style "font"
gtk-font-name = "Sans 8"

