# gtk application style

# theme
#gtk-theme-name = "Shiki-Brave"
gtk-theme-name = "Zukitwo"

# mouse cursor
gtk-cursor-theme-name = "ComixCursors-Slim-White"

# icons
gtk-icon-theme-name = "Faenza-Dark"

# font
style "font"
{
    font_name = "Sans 8"
}
widget_class "*" style "font"
gtk-font-name = "Sans 8"

